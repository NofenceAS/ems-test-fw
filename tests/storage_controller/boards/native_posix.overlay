&flashcontroller0 {
        reg = <0x00000000 DT_SIZE_K(4096)>;
};

&flash0 {
        reg = <0x00000000 DT_SIZE_K(4096)>;
        partitions {
                compatible = "fixed-partitions";
                #address-cells = <1>;
                #size-cells = <1>;

                partition@0 {
                        label = "log_partition";
                        reg = <0x00000000 0x00010000>;
                };
                partition@10000 {
                        label = "ano_partition";
                        reg = <0x00010000 0x000F0000>;
                };
        };
};