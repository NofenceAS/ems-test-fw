/ {
	aliases {
		gnss = &gnss;
		gnssuart = &gnssuart;
		dummy-extint = &gpio0;
	};
	chosen {
          zephyr,nofence = &gpio0;
        };


};


&gpio0 {
	status = "okay";
};

gnssuart: &uart0 {
	compatible = "nofence,mock-uart";
	gnss: mia_m10 {
		compatible = "u-blox,mia-m10";
		label = "ublox-mia-m10";
		status = "okay";
		extint-gpios = <&gpio0 27 GPIO_ACTIVE_HIGH>;
	};
};

